{"remainingRequest":"/Users/zhaoxiaoli/learn/my-learning-path/node_modules/babel-loader/lib/index.js!/Users/zhaoxiaoli/learn/my-learning-path/node_modules/eslint-loader/index.js??ref--13-0!/Users/zhaoxiaoli/learn/my-learning-path/src/utils/create.js","dependencies":[{"path":"/Users/zhaoxiaoli/learn/my-learning-path/src/utils/create.js","mtime":1584716534000},{"path":"/Users/zhaoxiaoli/learn/my-learning-path/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/zhaoxiaoli/learn/my-learning-path/node_modules/babel-loader/lib/index.js","mtime":499162500000},{"path":"/Users/zhaoxiaoli/learn/my-learning-path/node_modules/eslint-loader/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["/Users/zhaoxiaoli/learn/my-learning-path/src/utils/create.js"],"names":["Vue","create","Component","props","vm","render","h","$mount","document","body","appendChild","$el","comp","$children","remove","removeChild","$destroy"],"mappings":"AAAA,OAAOA,GAAP,MAAgB,KAAhB,C,CAEA;AACA;;AACA,OAAO,SAASC,MAAT,CAAgBC,SAAhB,EAA2BC,KAA3B,EAAkC;AACvC;AACA,MAAMC,EAAE,GAAG,IAAIJ,GAAJ,CAAQ;AACjBK,IAAAA,MADiB,kBACVC,CADU,EACP;AACR;AACA,aAAOA,CAAC,CAACJ,SAAD,EAAY;AAACC,QAAAA,KAAK,EAALA;AAAD,OAAZ,CAAR;AACD;AAJgB,GAAR,CAAX,CAFuC,CASvC;;AACAC,EAAAA,EAAE,CAACG,MAAH,GAVuC,CAYvC;;AACAC,EAAAA,QAAQ,CAACC,IAAT,CAAcC,WAAd,CAA0BN,EAAE,CAACO,GAA7B,EAbuC,CAevC;;AACA,MAAMC,IAAI,GAAGR,EAAE,CAACS,SAAH,CAAa,CAAb,CAAb,CAhBuC,CAkBvC;;AACAD,EAAAA,IAAI,CAACE,MAAL,GAAc,YAAM;AAClBN,IAAAA,QAAQ,CAACC,IAAT,CAAcM,WAAd,CAA0BX,EAAE,CAACO,GAA7B;AACAC,IAAAA,IAAI,CAACI,QAAL;AACD,GAHD;;AAKA,SAAOJ,IAAP;AACD","sourcesContent":["import Vue from 'vue'\r\n\r\n// 创建一个create函数，可以动态生成组件实例，并且挂载至body上\r\n// Component:是组件配置对象\r\nexport function create(Component, props) {\r\n  // 借用Vue构造函数来动态生成这个组件实例\r\n  const vm = new Vue({\r\n    render(h) {\r\n      // h是createElement别名，它可以返回一个虚拟dom，VNode\r\n      return h(Component, {props})\r\n    }\r\n  })\r\n\r\n  // 通过挂载得到真实dom\r\n  vm.$mount()\r\n\r\n  // 通过$el属性获取真实dom\r\n  document.body.appendChild(vm.$el)\r\n\r\n  // 组件实例返回\r\n  const comp = vm.$children[0]\r\n\r\n  // 销毁方法\r\n  comp.remove = () => {\r\n    document.body.removeChild(vm.$el)\r\n    comp.$destroy()\r\n  }\r\n\r\n  return comp\r\n}"]}]}