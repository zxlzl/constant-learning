 \"@/components/form/KFormItem.vue\";\r\nimport KForm from \"@/components/form/KForm.vue\";\r\nimport Notice from '@/components/Notice.vue';\r\n\r\nexport default {\r\n  components: {\r\n    ElementTest,\r\n    KInput,\r\n    KFormItem,\r\n    KForm\r\n  },\r\n  data() {\r\n    return {\r\n      model: {\r\n        username: \"\",\r\n        password: \"\"\r\n      },\r\n      rules: {\r\n        username: [{ required: true, message: \"请输入用户名称\" }],\r\n        password: [{ required: true, message: \"请输入密码\" }]\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    onLogin() {\r\n      // 全局校验\r\n      this.$refs.loginForm.validate(isValid => {\r\n        if (isValid) {\r\n          console.log('submit login!');\r\n          \r\n        }else {\r\n          // alert('校验失败')\r\n          this.$create(Notice, {\r\n            title: '校验失败',\r\n            message: '校验错误，请重试！',\r\n            duration: 3000\r\n          }).show()\r\n        }\r\n      })\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>"]}]}